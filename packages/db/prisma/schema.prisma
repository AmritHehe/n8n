generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model user{ 
  id   String @id @default(uuid())
  name String  @unique
  pass String 
}
model workflow{ 
  id Int @id @default(autoincrement())
  title String
  nodes String
  Connections String
}
model credentials{ 
  id Int @id @default(autoincrement())
  title String 
  platform String
  data String
}
